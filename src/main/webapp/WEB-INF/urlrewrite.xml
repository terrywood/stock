<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 3.0//EN"
          "http://tuckey.org/res/dtds/urlrewrite3.0.dtd">
<urlrewrite>

    <rule>
        <from>^/admin/image/(.*)$</from>
        <to>/admin/image.do?path=/$1</to>
    </rule>

    <rule>
        <from>^/admin/floorPlan/imageView/(.*)$</from>
        <to>/admin/floorPlan/imageView.do?path=/$1</to>
    </rule>
    <rule>
        <from>^/common/download/(.*)$</from>
        <to>/common/download.do?path=/$1</to>
    </rule>

    <rule>
        <from>^/common/barCode/(.*)$</from>
        <to>/api/barCode.do?code=$1</to>
    </rule>

	<rule>
		 <from>^/api/(.*).xml</from>  
		 <to>/api/$1.do</to>
	</rule>
	
	<rule>
		 <from>^/api/image/(.*).(jpg|png|gif|bmp)$</from>  
		 <to>/api/image.do?fileName=$1.$2</to>
	</rule>
	

</urlrewrite>  
